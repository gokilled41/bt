    version="7.0">
    <security-domain xmlns="http://www.jboss.com/xml/ns/javaee">VTBA-domain</security-domain>
    <loader-repository xmlns="http://www.jboss.com/xml/ns/javaee">
        vtm3oserver:loader=vtm3oserver.ear
        <loader-repository-config>
            java2ParentDelegation=false
        </loader-repository-config>
    </loader-repository>
	<module>
		<ejb>M3OServerApplication-ejb.jar</ejb>
	</module>
	<module>
		<ejb>BPMNProcessControl-ejb.jar</ejb>
	</module>
	<module>
		<ejb>DynamicMDB-ejb.jar</ejb>
	</module>
	<module>
		<ejb>M3OAuditLogger-ejb.jar</ejb>
	</module>
	<module>
		<ejb>SimpleServiceQuery-ejb.jar</ejb>
	</module>
		<module>
		<ejb>Workflow-ejb.jar</ejb>
	</module>
    <!--
    <module xmlns="http://www.jboss.com/xml/ns/javaee">
        <service>res/vtStartup-service.xml</service>
    </module>
    -->
    <!--
    <module xmlns="http://www.jboss.com/xml/ns/javaee">
        <service>vtm3ocommands.sar</service>
    </module>
    -->    
	<module>
		<web>
			<web-uri>vtaxis2.war</web-uri>
			<context-root>vtservices</context-root>
		</web>
	</module>
    <!-- BA startup service -->
    <module xmlns="http://www.jboss.com/xml/ns/javaee">
        <service>M3OServerApplicationStartup-ejb.jar</service>
    </module>    
	<module>
		<web>
			<web-uri>vtm3oautostart.war</web-uri>
		</web>
	</module>
	<module>
		<web>
			<web-uri>vtrest4feed.war</web-uri>
			<context-root>feedserver</context-root>
		</web>
	</module>
</jboss:jboss-app>
