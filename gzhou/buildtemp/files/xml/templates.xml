<?xml version="1.0" encoding="UTF-8"?>
<templates>
	<template autoinsert="true" context="java" deleted="false"
		description="log error with commons logging" enabled="true"
		name="lerror">
		if(${log:var(org.apache.commons.logging.Log)}.isErrorEnabled())
		{&#13;
		${log:var(org.apache.commons.logging.Log)}.error(${message});&#13;
		}
	</template>
