@echo off

if "%1" == "" goto a1
if expandt "%1" == "" goto b

: a1
echo kk from [to]
goto end

: b

if expandt "%1" == "" set kkfrom=%1
set kkto=%1
if expandt "%2" == "" set kkto=%2

call afu /l%kkfrom%-%kkto% ar "expand lines" "full lines"
call afu /l%kkfrom%-%kkto% ar ExpandLines FullLines
call afu /l%kkfrom%-%kkto% ar "Expand Lines" "Full Lines"
call afu /l%kkfrom%-%kkto% ar expandLines fullLines
call afu /l%kkfrom%-%kkto% ar npr fpr

goto end

: end
